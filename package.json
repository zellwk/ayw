{
  "name": "learnjavascript.today",
  "version": "0.2.1",
  "description": "Learn JavaScript website (https://learnjavascript.today)",
  "author": "Zell Liew",
  "license": "ISC",
  "scripts": {
    "start": "node pm2.js",
    "restart": "pm2 restart learnjavascript.today",
    "server": "nodemon start.js --ignore 'src/**/*.js' --ignore 'dev/**/*' --ignore 'dist/**/*'",
    "dev": "NODE_ENV=development gulp",
    "eleventy": "rm -rf dist && eleventy --watch --config=eleventy.js --quiet",
    "eleventy-debug": "DEBUG=Eleventy* eleventy",
    "build": "npm rebuild node-sass && NODE_ENV=production gulp build",
    "observe": "NODE_ENV=production npm run build & gulp serve",
    "deploy": "npm run build && gulp deploy",
    "deployCI": "gulp deployCI",
    "test": "standard | standard-reporter --stylish"
  },
  "standard": {
    "ignore": [
      "**/prism.js"
    ]
  },
  "devDependencies": {
    "@11ty/eleventy": "^0.11.0",
    "@babel/core": "^7.16.12",
