    "babel-polyfill": "^6.23.0",
    "browser-sync": "^2.27.7",
    "count-to-date": "^1.0.2",
    "del": "^5.1.0",
    "front-matter": "^4.0.2",
    "gulp": "^4.0.2",
    "gulp-autoprefixer": "^7.0.1",
    "gulp-cli": "^2.3.0",
    "gulp-cssnano": "^2.1.2",
    "gulp-if": "^3.0.0",
    "gulp-imagemin": "^7.1.0",
    "gulp-newer": "^1.4.0",
    "gulp-notify": "^3.0.0",
    "gulp-plumber": "^1.0.1",
    "gulp-postcss": "^9.0.1",
    "gulp-rename": "^2.0.0",
    "gulp-rev": "^9.0.0",
    "gulp-rsync": "0.0.9",
    "gulp-sass": "^5.1.0",
    "gulp-sass-lint": "^1.1.1",
